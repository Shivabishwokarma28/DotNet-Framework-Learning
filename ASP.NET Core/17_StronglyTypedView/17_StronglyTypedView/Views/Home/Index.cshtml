@model List< _17_StronglyTypedView.Models.UserData>
@*@model IEnumerable< _17_StronglyTypedView.Models.UserData>*@
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<style>
    table {
        border-collapse: collapse;
        margin-top: 20px;
    }

    th, td {
        border: 1px solid gray;
        padding: 12px 25px;
        
    }
    
</style>
<table>
    <tr>
        <th>UserId</th>
        <th>UserName</th>
        <th>Email</th>
    </tr>
    
        

@{
    foreach (var a in Model)
    
    {
                 <tr>
                  <td>  <h2>@a.UserId</h2></td>
                <td><h2>@a.UserName</h2></td>
                <td><h2>@a.Email</h2></td>  
                </tr>
    }
}
</table>

